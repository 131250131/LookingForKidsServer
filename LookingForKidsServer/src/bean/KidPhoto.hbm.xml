<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.KidPhoto" table="kidsPhotos" catalog="lookingforkidsdb">
		<id column="photoID" name="photoID" type="integer" length="11">
			<generator class="identity"/>
		</id> 
		<property name ="kidID" column="kidID" type="integer"/>
		<property name ="photoPath" column="photoPath" type="string"/>
	</class>
</hibernate-mapping>